        <!--<servlet-class>mypack.Index</servlet-class>-->
    <!--</servlet>-->
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>mypack.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Gallery</servlet-name>
        <servlet-class>mypack.Gallery</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>mypack.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Profile</servlet-name>
        <servlet-class>mypack.Profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Exhibition</servlet-name>
        <servlet-class>mypack.Exhibition</servlet-class>
    </servlet>

    <!--<servlet-mapping>-->
        <!--<servlet-name>Index</servlet-name>-->
        <!--<url-pattern>/</url-pattern>-->
    <!--</servlet-mapping>-->
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Profile</servlet-name>
        <url-pattern>/myprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Exhibition</servlet-name>
        <url-pattern>/exhibition</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>Gallery</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>


</web-app>